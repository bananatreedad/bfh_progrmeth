<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="155.0" text="Name" />
          <TableColumn prefWidth="92.0" text="Entry" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <Button mnemonicParsing="false" text="add entry" BorderPane.alignment="CENTER_RIGHT">
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin></Button>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <HBox spacing="10.0">
         <children>
            <TextField fx:id="tefi_accountName" />
            <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="72.0" text="save" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
